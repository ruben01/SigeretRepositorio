@model Sigeret.Equipo

@{
    ViewBag.Title = "Editar";
}

<h2>Editar</h2>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Equipo</legend>



        @Html.HiddenFor(model => model.Id)

        <div class="editor-label">
            @Html.LabelFor(model => model.ModeloEquipo.Nombre)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ModeloEquipo.Nombre)
            @Html.ValidationMessageFor(model => model.ModeloEquipo.Nombre)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.Serie)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Serie)
            @Html.ValidationMessageFor(model => model.Serie)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.ModeloEquipo.Modelo)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ModeloEquipo.Modelo)
            @Html.ValidationMessageFor(model => model.ModeloEquipo.Modelo)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.ModeloEquipo.MarcaEquipo.Marca)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ModeloEquipo.MarcaEquipo.Marca)
            @Html.ValidationMessageFor(model => model.ModeloEquipo.MarcaEquipo.Marca)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.ModeloEquipo.Descripcion)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.ModeloEquipo.Descripcion)
            @Html.ValidationMessageFor(model => model.ModeloEquipo.Descripcion)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.IdEstatusEquipo)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.IdEstatusEquipo)
            @Html.ValidationMessageFor(model => model.IdEstatusEquipo)
        </div>



        <p>
            <input type="submit" value="Guardar" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Regresar", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
