@model IEnumerable<Sigeret.Solicitud>

@{
    ViewBag.Title = "Reporte Solicitudes Adm";
}

<h2>Reporte Solicitudes Administrador</h2>

<p>
    @Html.ActionLink("Nueva", "Nueva")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.HoraInicio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HoraFin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdUserProfile)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdLugar)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.HoraInicio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HoraFin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IdUserProfile)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IdLugar)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Detalles", "Detalles", new { id=item.Id }) |
            @Html.ActionLink("Cancelar", "Cancelar", new { id=item.Id })
        </td>
    </tr>
}

</table>
